<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.socialracing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SocialRacing">
        <activity
            android:name=".FinalStats"
            android:label="@string/title_activity_final_stats"
            android:theme="@style/Theme.SocialRacing.NoActionBar"></activity>
        <activity android:name=".CompleteActivity" />
        <activity android:name=".RaceTurnActivity" />
        <activity android:name=".WaitingActivity" />
        <activity
            android:name=".Runconfig"
            android:label="@string/title_activity_runconfig" />
        <activity android:name=".jointeam" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>